<div class="col-md-4 col-sm-4">
	<div class="panel panel-primary">
		<div class="panel-heading">
			<h3 class="panel-title" style="font-weight: bold"><%= link_to list.occasion, list_path(list.id) %></h3>
		</div>
		<div class="panel-body">
			<p>
				<%= list.owner.full_name if !list.owner?(session_user) %>
				<%= list.occasion_of if list.owner?(session_user) %>
			</p>
		</div>
	</div>
</div>